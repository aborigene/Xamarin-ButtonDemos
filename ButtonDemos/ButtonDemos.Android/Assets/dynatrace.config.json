{
	"android": {
		"autoStart": {
			"beaconUrl": "https://<url>.bf.dynatrace.com/mbeacon",
			"applicationId": "<appd id>"
		},
		"userOptIn": false,
		"agentBehavior": {
			"startupLoadBalancing":  true
		}
	},
	"ios": {
		"DTXApplicationId": "<app id>",
		"DTXBeaconUrl": "https://<url>.bf.dynatrace.com/mbeacon",
		"DTXUserOptIn": false,
		"DTXStartupLoadBalancing": true
	}
}
